            sh:order 19 ;
            sh:path kgcl_schema:has_undo ],
        [ sh:description "The representation of a node (URI, CURIE, label) " ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path kgcl_schema:about_node_representation ],
        [ sh:description "The language tag of a literal" ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path kgcl_schema:language ],
        [ sh:maxCount 1 ;
            sh:order 12 ;
            sh:path <rdf:type> ],
        [ sh:description "The value of a property held in the new instance of the ontology" ;
            sh:maxCount 1 ;
            sh:order 4 ;
            sh:path kgcl_schema:new_value ],
        [ sh:maxCount 1 ;
            sh:order 11 ;
            sh:path <https://w3id.org/kgcl_schema/basics/id> ],
        [ sh:class <om:OntologySubset> ;
            sh:description "The subset that pertains to this change" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 20 ;
            sh:path kgcl_schema:in_subset ],
        [ sh:description "The old datatype of a literal" ;
            sh:maxCount 1 ;
            sh:order 10 ;
            sh:path kgcl_schema:old_datatype ],
        [ sh:maxCount 1 ;
            sh:order 15 ;
            sh:path kgcl_schema:pull_request ],
        [ sh:class <om:Node> ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 0 ;
            sh:path kgcl_schema:about_node ],
        [ sh:class <http://www.w3.org/ns/prov#Activity> ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 13 ;
            sh:path <http://www.w3.org/ns/prov#wasGeneratedBy> ],
        [ sh:description "The value of a property held in the old instance of the ontology" ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path kgcl_schema:old_value ],
        [ sh:description "The old language tag of a literal" ;
            sh:maxCount 1 ;
            sh:order 8 ;
            sh:path kgcl_schema:old_language ],
        [ sh:description "The type (IRI or Literal) of an old value" ;
            sh:maxCount 1 ;
            sh:order 5 ;
            sh:path kgcl_schema:old_value_type ],
        [ sh:maxCount 1 ;
            sh:order 16 ;
            sh:path <dcterms:creator> ],
        [ sh:maxCount 1 ;
            sh:order 17 ;
            sh:path <dcterms:date> ],
