        kgcl_schema:Configuration,
        kgcl_schema:Session,
        kgcl_schema:TextualDiff ;
    dcterms:license "MIT" ;
    dcterms:title "kgcl-schema" ;
    rdfs:seeAlso "https://hrshdhgd.github.io/kgcl-schema" ;
    linkml:generation_date "2022-09-14T16:13:49" ;
    linkml:metamodel_version "1.7.0" ;
    linkml:source_file "kgcl_schema.yaml" ;
    linkml:source_file_date "2022-09-14T15:15:55" ;
    linkml:source_file_size 29984 .

kgcl_schema:AddNodeToSubset a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "add node to subset" ;
    rdfs:seeAlso "http://wiki.geneontology.org/index.php/Adding_a_Term_to_a_GO_Subset_(Slim)" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass <http://w3id.org/kgcl_schema/om/OntologySubset> ;
            owl:onProperty kgcl_schema:in_subset ],
        kgcl_schema:AddToSubset,
        kgcl_schema:NodeChange ;
    skos:altLabel "add term to slim" ;
    skos:definition "Places a node inside a subset, by annotating that node" .

kgcl_schema:ClassCreation a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "class creation" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty kgcl_schema:change_description ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass <http://w3id.org/kgcl_schema/om/Node> ;
            owl:onProperty kgcl_schema:superclass ],
        kgcl_schema:NodeCreation ;
    skos:definition "A node creation where the owl type is 'class'" .

kgcl_schema:DatatypeChange a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "datatype change" ;
    rdfs:subClassOf kgcl_schema:DatatypeOrLanguageTagChange .

kgcl_schema:EdgeLogicalInterpretationChange a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "edge logical interpretation change" ;
    rdfs:subClassOf kgcl_schema:EdgeChange ;
    skos:definition "An edge change where the subjet, object, and predicate are unchanged, but the logical interpretation changes" ;
    linkml:examples "Example(value='changing a SubClassOf r some b to a subClassOf r only b', description=None)" .

kgcl_schema:EdgeObsoletion a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "edge obsoletion" ;
    rdfs:seeAlso "https://mondo.readthedocs.io/en/latest/editors-guide/g-logical-axioms/#excluded-subclassof" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass <http://w3id.org/kgcl_schema/om/Annotation> ;
            owl:onProperty <http://w3id.org/kgcl_schema/om/annotation_set> ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass <http://w3id.org/kgcl_schema/om/Node> ;
            owl:onProperty <http://w3id.org/kgcl_schema/om/subject> ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass <http://w3id.org/kgcl_schema/om/Node> ;
            owl:onProperty <http://w3id.org/kgcl_schema/om/object> ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass <http://w3id.org/kgcl_schema/om/Node> ;
            owl:onProperty <http://w3id.org/kgcl_schema/om/predicate> ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty kgcl_schema:change_description ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass <http://w3id.org/kgcl_schema/om/OntologyElement> ;
            owl:onProperty kgcl_schema:about ],
        kgcl_schema:EdgeChange,
        kgcl_schema:Obsoletion ;
    skos:altLabel "relationship obsoletion" ;
    skos:definition "An edge change in which an edge is obsoleted." ;
    skos:note "Note there is not yet consensus in how this should be done in rdf/owl implementations. Simply marking the edge deprecated with render it visible to most clients. Care must be taken because simply adding a deprecation tag to an axiom does not silence it -- it is still asserted. It is therefore more common to obsolete an edge by shadowing the axiom as an annotation. See the Mondo docs for example" .

kgcl_schema:EdgeRewiring a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "edge rewiring" ;
    rdfs:subClassOf kgcl_schema:EdgeChange ;
    skos:definition "An edge change where one node is replaced with another, as in the case of obsoletion with replacement" ;
    linkml:examples "Example(value='changing G1 annotated-to T1 to G1 annotated-to T2, after obsoletion of T1 and replacement with T2', description=None)" .

kgcl_schema:LogicalAxiomChange a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "logical axiom change" ;
    rdfs:subClassOf kgcl_schema:SimpleChange ;
    skos:definition """A simple change where a logical axiom is changed, where the logical axiom cannot be represented as an edge

  """ .

kgcl_schema:MappingCreation a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "mapping creation" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass <http://w3id.org/kgcl_schema/om/Node> ;
            owl:onProperty <http://w3id.org/kgcl_schema/om/subject> ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass <http://w3id.org/kgcl_schema/om/Node> ;
            owl:onProperty <http://w3id.org/kgcl_schema/om/predicate> ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass <http://w3id.org/kgcl_schema/om/Node> ;
            owl:onProperty <http://w3id.org/kgcl_schema/om/object> ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty kgcl_schema:change_description ],
        kgcl_schema:Creation,
        kgcl_schema:EdgeCreation ;
    skos:altLabel "xref creation" ;
    skos:definition "A specific kind of edge creation in which the created edge is a mapping." .

kgcl_schema:MultiNodeObsoletion a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "multi node obsoletion" ;
    rdfs:seeAlso "https://github.com/cmungall/obo-scripts/blob/master/obo-obsoletify.pl" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom kgcl_schema:NodeObsoletion ;
            owl:onProperty kgcl_schema:change_set ],
        [ a owl:Restriction ;
            owl:allValuesFrom kgcl_schema:Change ;
            owl:onProperty kgcl_schema:associated_change_set ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty kgcl_schema:change_description ],
        kgcl_schema:ComplexChange ;
    skos:altLabel "multi class obsoletion",
        "multi concept obsoletion",
        "multi node deprecation",
        "multi term obsoletion" ;
    skos:definition "A complex change consisting of multiple obsoletions." .

kgcl_schema:NameBecomesSynonym a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "name becomes synonym" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass kgcl_schema:NewSynonym ;
            owl:onProperty kgcl_schema:change_2 ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass kgcl_schema:NodeRename ;
            owl:onProperty kgcl_schema:change_1 ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty kgcl_schema:change_description ],
        kgcl_schema:NodeSynonymChange ;
    skos:definition "A node synonym where the name NAME of an node NODE moves to a synonym, and NODE receives a new name. This change consists of compose of (1) a node rename where NAME is replaced by a different name (2) a new synonym" .

kgcl_schema:NewTextDefinition a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "new text definition" ;
    rdfs:subClassOf kgcl_schema:NodeTextDefinitionChange ;
    skos:definition "A node change where a de-novo text definition is created" .

kgcl_schema:NodeAnnotationReplacement a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "node annotation replacement" ;
    rdfs:subClassOf kgcl_schema:NodeAnnotationChange ;
    skos:definition "A node annotation change where the change replaces a particular property value. TODO" .

kgcl_schema:NodeDeepening a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "node deepening" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty kgcl_schema:change_description ],
        kgcl_schema:NodeMove ;
    skos:definition "A node move in which a node where the destination is a proper descendant of the original location. Note that here descendant applied not just to subclass, but edges of any predicate in the relational graph" ;
    linkml:examples "Example(value='changing a is-a b to a is-a c, where c is a subclass of b', description=None)" .

kgcl_schema:NodeDeletion a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "node deletion" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty kgcl_schema:change_description ],
        kgcl_schema:Deletion,
        kgcl_schema:NodeChange ;
    skos:definition "Deletion of a node from the graph. Note it is recommended nodes are obsoleted and never merged, but this operation exists to represent deletions in ontologies, accidental or otherwise" .

kgcl_schema:NodeDirectMerge a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "node direct merge" ;
    rdfs:seeAlso "http://wiki.geneontology.org/index.php/Merging_Ontology_Terms",
        "http://wiki.geneontology.org/index.php/Principles_for_merging_terms" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onClass <http://w3id.org/kgcl_schema/om/Node> ;
            owl:onProperty kgcl_schema:has_direct_replacement ;
            owl:qualifiedCardinality 1 ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass <http://w3id.org/kgcl_schema/om/Node> ;
            owl:onProperty kgcl_schema:about_node ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty kgcl_schema:change_description ],
        kgcl_schema:AllowsAutomaticReplacementOfEdges,
        kgcl_schema:NodeObsoletion ;
    skos:definition "An obsoletion change in which all metadata (including name/label) from the source node is deleted and added to the target node, and edges can automatically be rewired to point to the target node" ;
    skos:note "In the OBO format serialization of the graph, the source node vanishes from the file as a distinct entry and is retained only as an alt_id" .

kgcl_schema:NodeObsoletionWithDirectReplacement a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "node obsoletion with direct replacement" ;
    rdfs:seeAlso "http://wiki.geneontology.org/index.php/Merging_Ontology_Terms",
        "http://wiki.geneontology.org/index.php/Principles_for_merging_terms" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onClass <http://w3id.org/kgcl_schema/om/Node> ;
            owl:onProperty kgcl_schema:has_direct_replacement ;
            owl:qualifiedCardinality 1 ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty kgcl_schema:change_description ],
        kgcl_schema:AllowsAutomaticReplacementOfEdges,
        kgcl_schema:NodeObsoletion ;
    skos:definition "An obsoletion change in which information from the obsoleted node is selectively copied to a single target, and edges can automatically be rewired to point to the target node" .

kgcl_schema:NodeObsoletionWithNoDirectReplacement a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "node obsoletion with no direct replacement" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty kgcl_schema:change_description ],
        [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:allValuesFrom <http://w3id.org/kgcl_schema/om/Node> ;
                        owl:onProperty kgcl_schema:has_nondirect_replacement ] [ a owl:Restriction ;
                        owl:onProperty kgcl_schema:has_nondirect_replacement ;
                        owl:someValuesFrom <http://w3id.org/kgcl_schema/om/Node> ] ) ],
        kgcl_schema:NodeObsoletion ;
    skos:definition "An obsoletion change in which there is no direct replacement" .

kgcl_schema:NodeShallowing a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "node shallowing" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty kgcl_schema:change_description ],
        kgcl_schema:NodeMove ;
    skos:definition "The opposite of node deepening" ;
    linkml:examples "Example(value='changing a is-a c to a is-a b, where c is a subclass of b', description=None)" .

kgcl_schema:NodeUnobsoletion a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "node unobsoletion" ;
    rdfs:seeAlso "http://wiki.geneontology.org/index.php/Restoring_an_Obsolete_Ontology_Term" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass <http://w3id.org/kgcl_schema/om/Node> ;
            owl:onProperty kgcl_schema:consider ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass <http://w3id.org/kgcl_schema/om/Node> ;
            owl:onProperty kgcl_schema:replaced_by ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty kgcl_schema:change_description ],
        kgcl_schema:NodeChange,
        kgcl_schema:Unobsoletion ;
    skos:altLabel "class unobsoletion",
        "concept unobsoletion",
        "node undeprecation",
        "term unobsoletion" ;
    skos:definition "unobsoletion of a node deprecates usage of that node. Rarely applied." .

kgcl_schema:PlaceUnder a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "place under" ;
    rdfs:subClassOf kgcl_schema:EdgeCreation ;
    skos:definition "An edge creation where the predicate is owl:subClassOf" .

kgcl_schema:PredicateChange a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "predicate change" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty kgcl_schema:change_description ],
        kgcl_schema:EdgeChange ;
    skos:definition "An edge change where the predicate (relationship type) is modified." ;
    linkml:examples "Example(value='changing a is-a b to a part-of b', description=None)" .

kgcl_schema:RemoveSynonym a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "remove synonym" ;
    rdfs:subClassOf kgcl_schema:NodeSynonymChange ;
    skos:definition "A node synonym change where a synonym is deleted" .

kgcl_schema:RemoveTextDefinition a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "remove text definition" ;
    rdfs:subClassOf kgcl_schema:NodeTextDefinitionChange ;
    skos:definition "A node change where a text definition is deleted" .

kgcl_schema:RemoveUnder a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "remove under" ;
    rdfs:subClassOf kgcl_schema:EdgeDeletion ;
    skos:definition """An edge deletion where the predicate is owl:subClassOf
  """ .

kgcl_schema:RemovedNodeFromSubset a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "removed node from subset" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass <http://w3id.org/kgcl_schema/om/Node> ;
            owl:onProperty kgcl_schema:about_node ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty kgcl_schema:subset ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty kgcl_schema:change_description ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass <http://w3id.org/kgcl_schema/om/OntologySubset> ;
            owl:onProperty kgcl_schema:in_subset ],
        kgcl_schema:NodeChange,
        kgcl_schema:RemoveFromSubset ;
    skos:altLabel "remove term from slim" ;
    skos:definition "Removes a node from a subset, by removing an annotation" .

kgcl_schema:SetLanguageForName a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "set language for name" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty kgcl_schema:change_description ],
        kgcl_schema:LanguageTagChange,
        kgcl_schema:NodeChange ;
    skos:definition "A node change where the string value for the name is unchanged but the language tag is set" .

kgcl_schema:SynonymPredicateChange a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "synonym predicate change" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass kgcl_schema:TextualDiff ;
            owl:onProperty kgcl_schema:has_textual_diff ],
        kgcl_schema:NodeSynonymChange ;
    skos:altLabel "node scope change" ;
    skos:definition "A node synonym change where the predicate of a synonym is changed. Background: synonyms can be represented by a variety of predicates. For example, many OBO ontologies make use of predicates such as oio:hasExactSynonym, oio:hasRelatedSynonym, etc" .

kgcl_schema:SynonymReplacement a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "synonym replacement" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass kgcl_schema:TextualDiff ;
            owl:onProperty kgcl_schema:has_textual_diff ],
        kgcl_schema:NodeSynonymChange ;
    skos:definition "A node synonym change where the text of a synonym is changed" .

kgcl_schema:TextDefinitionReplacement a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "text definition replacement" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass kgcl_schema:TextualDiff ;
            owl:onProperty kgcl_schema:has_textual_diff ],
        kgcl_schema:NodeTextDefinitionChange ;
    skos:definition "A node change where a text definition is modified" .

kgcl_schema:Transaction a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "transaction" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom kgcl_schema:Change ;
            owl:onProperty kgcl_schema:change_set ],
        kgcl_schema:Change ;
    skos:definition "A change that is a composition of a set of changes, where those changes are treated as a single unit. Could be a single change, or the results of an ontology diff" .

kgcl_schema:has_replacement a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "has replacement" ;
    rdfs:domain kgcl_schema:NodeObsoletion ;
    rdfs:range <http://w3id.org/kgcl_schema/om/Node> ;
    rdfs:subPropertyOf kgcl_schema:target ;
    skos:definition "A single node obsoletion event may be accompanied by one or more suggested replacement nodes" ;
    skos:note "In OBO, the replacement term is indicated using IAO:nnnnnn" .

kgcl_schema:new_filler a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "new filler" ;
    rdfs:range linkml:String ;
    skos:definition "The new filler of an OWL restriction" .

kgcl_schema:new_property a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "new property" ;
    rdfs:range linkml:String ;
    skos:definition "The new property of an OWL restriction" .

kgcl_schema:new_subclass a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "new subclass" ;
    rdfs:range linkml:String ;
    skos:definition "The new subclass of a subsumption axiom" .

kgcl_schema:subclass a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "subclass" ;
    rdfs:range linkml:String ;
    skos:definition "The subclass of a subsumption axiom" .

kgcl_schema:subclass_type a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "subclass type" ;
    rdfs:range linkml:String ;
    skos:definition "The type of a subclass" .

kgcl_schema:superclass_type a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "superclass type" ;
    rdfs:range linkml:String ;
    skos:definition "The type of a superclass" .

linkml:SubsetDefinition a owl:Class ;
    rdfs:label "subset_definition" .

<http://w3id.org/kgcl_schema/om/filler> a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "filler" ;
    rdfs:range linkml:String .

<http://w3id.org/kgcl_schema/om/filler_type> a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "filler type" ;
    rdfs:range linkml:String .

<http://w3id.org/kgcl_schema/om/object_representation> a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "object representation" ;
    rdfs:range linkml:String .

<http://w3id.org/kgcl_schema/om/predicate_representation> a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "predicate representation" ;
    rdfs:range linkml:String .

<http://w3id.org/kgcl_schema/om/property> a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "property" ;
    rdfs:range <http://w3id.org/kgcl_schema/om/Node> .

<http://w3id.org/kgcl_schema/om/property_type> a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "property type" ;
    rdfs:range linkml:String .

<http://w3id.org/kgcl_schema/om/property_value_set> a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "property value set" ;
    rdfs:range <http://w3id.org/kgcl_schema/om/PropertyValue> .

<http://w3id.org/kgcl_schema/om/subject_representation> a owl:ObjectProperty,
