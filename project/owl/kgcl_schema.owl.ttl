            owl:onProperty <http://w3id.org/kgcl_schema/om/subject> ],
        kgcl_schema:Creation,
        kgcl_schema:EdgeChange ;
    skos:altLabel "relationship creation" ;
    skos:definition "An edge change in which a de-novo edge is created. The edge is potentially annotated in the same action." .

kgcl_schema:NewSynonym a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "new synonym" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty kgcl_schema:qualifier ],
        kgcl_schema:NodeSynonymChange ;
    skos:definition "A node synonym change where a de-novo synonym is created" .

kgcl_schema:NodeMove a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "node move" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty kgcl_schema:change_description ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty kgcl_schema:old_object_type ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty kgcl_schema:new_object_type ],
        kgcl_schema:EdgeChange ;
    skos:definition "A node move is a combination of deleting a parent edge and adding a parent edge, where the predicate is preserved and the object/parent node changes" ;
    linkml:examples "Example(value='changing a is-a b to a is-a c', description=None)" .

kgcl_schema:NodeRename a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "node rename" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty kgcl_schema:old_value ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty kgcl_schema:change_description ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty kgcl_schema:new_value ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass kgcl_schema:TextualDiff ;
            owl:onProperty kgcl_schema:has_textual_diff ],
        kgcl_schema:NodeChange ;
    skos:definition "A node change where the name (aka rdfs:label) of the node changes" ;
    linkml:examples "Example(value=\"rename UBERON:0002398 from 'manus' to 'hand'\", description=\"replacing the rdfs:label of 'manus' on an uberon class with the rdfs:label 'hand'\")" .

kgcl_schema:SubsetMembershipChange a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "subset membership change" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass <http://w3id.org/kgcl_schema/om/OntologySubset> ;
            owl:onProperty kgcl_schema:in_subset ],
        kgcl_schema:ChangeMixin,
        linkml:mixin ;
    skos:definition "A change in the membership status of a node with respect to a subset (view)" .

kgcl_schema:associated_change_set a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "associated change set" ;
    rdfs:range kgcl_schema:Change ;
    rdfs:subPropertyOf kgcl_schema:change_set ;
    skos:definition """All changes associated with a set of obsoletions. This change set is the composed change set rather than the set of individual changes. For example, if previous state is:
  `A subClassOf B subClassOf C subClassOf D`
and we obsolete {B,C}, then the individual changes are `A moves from B to C` and `B moves from C to D`, but the composed change set is `A moves from B to D`""" .

kgcl_schema:language a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "language" ;
    rdfs:range <https://w3id.org/kgcl_schema/basics/LanguageTag> ;
    skos:definition "The language tag of a literal" .

kgcl_schema:predicate_type a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "predicate type" ;
    rdfs:range linkml:String ;
    skos:definition "The type (IRI or Literal) of an edge's subject" .

kgcl_schema:subject_type a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "subject type" ;
    rdfs:range linkml:String ;
    skos:definition "The type (IRI or Literal) of an edge's subject" .

linkml:TypeDefinition a owl:Class ;
    rdfs:label "type_definition" .

<http://w3id.org/kgcl_schema/om/PropertyValue> a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "property value" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass <http://w3id.org/kgcl_schema/om/Node> ;
            owl:onProperty <http://w3id.org/kgcl_schema/om/property> ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty <http://w3id.org/kgcl_schema/om/filler> ],
        <http://w3id.org/kgcl_schema/om/OntologyElement> ;
    skos:definition "a property-value pair" .

<http://w3id.org/kgcl_schema/om/SynonymScopeEnum> a owl:Class,
        linkml:EnumDefinition ;
    rdfs:label "synonym_scope_enum" ;
    owl:unionOf ( OIO:hasNarrowSynonym OIO:hasBroadSynonym OIO:hasNarrowSynonym OIO:hasExactSynonym ) ;
    linkml:permissible_values OIO:hasBroadSynonym,
        OIO:hasExactSynonym,
        OIO:hasNarrowSynonym .

OIO:hasNarrowSynonym a <http://w3id.org/kgcl_schema/om/SynonymScopeEnum>,
        owl:Class ;
    rdfs:label "narrow",
        "related" .

owl:NamedIndividual a <http://w3id.org/kgcl_schema/om/OwlTypeEnum>,
        owl:Class ;
    rdfs:label "NAMED_INDIVIDUAL" .

<http://www.w3.org/ns/prov#wasInformedBy> a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "was informed by" ;
    rdfs:range <http://www.w3.org/ns/prov#Activity> ;
    skos:exactMatch <http://www.w3.org/ns/prov#wasInformedBy> .

kgcl_schema:Creation a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "creation" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass kgcl_schema:Deletion ;
            owl:onProperty kgcl_schema:has_undo ],
        kgcl_schema:ChangeMixin,
        linkml:mixin ;
    skos:definition "Creation of an element." .

kgcl_schema:Deletion a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "deletion" ;
    rdfs:subClassOf kgcl_schema:ChangeMixin,
        linkml:mixin ;
    skos:definition "Removal of an element." ;
    skos:note "In general, for OBO ontologies, node elements should never be deleted, always obsolete. However, edge deletion is more common." .

kgcl_schema:NodeTextDefinitionChange a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "node text definition change" ;
    rdfs:subClassOf kgcl_schema:NodeChange ;
    skos:definition "A node change where the text definition is changed" .

kgcl_schema:SimpleChange a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "simple change" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty kgcl_schema:old_value ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty kgcl_schema:old_value_type ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty kgcl_schema:old_datatype ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty kgcl_schema:new_datatype ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty kgcl_schema:old_language ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty kgcl_schema:new_language ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty kgcl_schema:new_value_type ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty kgcl_schema:new_value ],
        kgcl_schema:Change ;
    skos:altLabel "atomic change" ;
    skos:definition "A change that is about a single ontology element" .

kgcl_schema:about_node a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "about node" ;
    rdfs:range <http://w3id.org/kgcl_schema/om/Node> ;
    rdfs:subPropertyOf kgcl_schema:about .

kgcl_schema:has_direct_replacement a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "has direct replacement" ;
    rdfs:range <http://w3id.org/kgcl_schema/om/Node> ;
    skos:closeMatch IAO:0100001 ;
    skos:definition "An obsoletion replacement where it IS valid to automatically update annotations/edges pointing at the node with its direct replacement" ;
    skos:note "if a node obsoletion C C about N1, and Chas direct replacement N2, then N1 IAO:0100001 N2",
        "if a node obsoletion C, C about N1, and C has direct replacement N2, and there exists an edge with an edge property referencing N1, it is possible to replace with N2" .

kgcl_schema:has_nondirect_replacement a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "has nondirect replacement" ;
    rdfs:range <http://w3id.org/kgcl_schema/om/Node> ;
    skos:closeMatch OIO:consider ;
    skos:definition "An obsoletion replacement where it is NOT valid to automatically update annotations/edges pointing at the node with its direct replacement" ;
    skos:note "if a node obsoletion C C about N1, and Chas direct replacement N2, then N1 oio:consider N2",
        "if a node obsoletion C, C about N1, and C has direct replacement N2, and there exists an edge with an edge property referencing N1, it is possible to replace with N2" .

kgcl_schema:new_value a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "new value" ;
    rdfs:range linkml:String ;
    skos:definition "The value of a property held in the new instance of the ontology" .

kgcl_schema:old_value a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "old value" ;
    rdfs:range linkml:String ;
    skos:definition "The value of a property held in the old instance of the ontology" .

kgcl_schema:has_textual_diff a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "has textual diff" ;
    rdfs:domain kgcl_schema:Change ;
    rdfs:range kgcl_schema:TextualDiff ;
    skos:definition "A representation of character-level changes on a textual literal property. For example, if a text definition may change by only a single character such as addition of a period, it is useful to be able to see this visually." ;
    skos:note "this is NOT for representing the diff of the change as a whole." .

<https://w3id.org/kgcl_schema/basics/id> a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "id" ;
    rdfs:range linkml:String .

<http://w3id.org/kgcl_schema/om/object> a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "object" ;
    rdfs:range <http://w3id.org/kgcl_schema/om/Node> .

<http://w3id.org/kgcl_schema/om/predicate> a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "predicate" ;
    rdfs:range <http://w3id.org/kgcl_schema/om/Node> .

kgcl_schema:NodeObsoletion a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "node obsoletion" ;
    rdfs:seeAlso "http://wiki.geneontology.org/index.php/Obsoleting_an_Existing_Ontology_Term" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty kgcl_schema:change_description ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass <http://w3id.org/kgcl_schema/om/OntologyElement> ;
            owl:onProperty kgcl_schema:about ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://w3id.org/kgcl_schema/om/Node> ;
            owl:onProperty kgcl_schema:has_nondirect_replacement ],
        [ a owl:Restriction ;
            owl:allValuesFrom kgcl_schema:Change ;
            owl:onProperty kgcl_schema:associated_change_set ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass <http://w3id.org/kgcl_schema/om/Node> ;
            owl:onProperty kgcl_schema:has_direct_replacement ],
        kgcl_schema:NodeChange,
        kgcl_schema:Obsoletion ;
    skos:altLabel "class obsoletion",
        "concept obsoletion",
        "node deprecation",
        "term obsoletion" ;
    skos:definition "Obsoletion of a node deprecates usage of that node, but does not delete it." .

kgcl_schema:NodeSynonymChange a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "node synonym change" ;
    rdfs:subClassOf kgcl_schema:NodeChange .

kgcl_schema:Obsoletion a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "obsoletion" ;
    rdfs:seeAlso "http://wiki.geneontology.org/index.php/Obsoleting_an_Existing_Ontology_Term",
        "https://mondo.readthedocs.io/en/latest/editors-guide/merging-and-obsoleting/" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass kgcl_schema:Obsoletion ;
            owl:onProperty kgcl_schema:has_undo ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass <http://w3id.org/kgcl_schema/om/OntologyElement> ;
            owl:onProperty kgcl_schema:about ],
        kgcl_schema:ChangeMixin,
        linkml:mixin ;
    skos:altLabel "deprecation",
        "retiring" ;
    skos:definition "Obsoletion of an element deprecates usage of that element, but does not delete that element." ;
    skos:note "In OWL, deprecation is handled by the deprecation axiom. In OBO, we use this, but place additional requirements and expectations on obsolete elements" .

kgcl_schema:about a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "about" ;
    rdfs:range <http://w3id.org/kgcl_schema/om/OntologyElement> ;
    skos:definition "The 'focus' entity on which the change operates" .

kgcl_schema:change_set a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "change set" ;
    rdfs:range kgcl_schema:Change ;
    skos:definition "A collection of changes" .

kgcl_schema:has_undo a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "has undo" ;
    rdfs:domain kgcl_schema:Change ;
    rdfs:range kgcl_schema:Change ;
    skos:definition "A change that reverses this change" .

kgcl_schema:in_subset a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "in subset" ;
    rdfs:range <http://w3id.org/kgcl_schema/om/OntologySubset> ;
    skos:definition "The subset that pertains to this change" .

<https://w3id.org/kgcl_schema/basics/LanguageTag> a owl:Class,
        linkml:TypeDefinition ;
    rdfs:label "language tag" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onDataRange <http://example.org/UNKNOWN/xml/lang> ;
            owl:onProperty linkml:topValue ;
            owl:qualifiedCardinality 1 ] .

<http://w3id.org/kgcl_schema/om/OntologySubset> a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "ontology subset" ;
    rdfs:subClassOf <http://w3id.org/kgcl_schema/om/OntologyElement> .

<http://w3id.org/kgcl_schema/om/OwlTypeEnum> a owl:Class,
        linkml:EnumDefinition ;
    rdfs:label "owl_type_enum" ;
    owl:unionOf ( owl:Class owl:ObjectProperty owl:NamedIndividual ) ;
    linkml:permissible_values owl:Class,
        owl:NamedIndividual,
        owl:ObjectProperty .

<http://w3id.org/kgcl_schema/om/subject> a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "subject" ;
    rdfs:range <http://w3id.org/kgcl_schema/om/Node> .

kgcl_schema:TextualDiff a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "textual diff" ;
    skos:definition "A summarizing of a change on a piece of text. This could be rendered in a number of different ways" .

<http://w3id.org/kgcl_schema/om/annotation_set> a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "annotation set" ;
    rdfs:range <http://w3id.org/kgcl_schema/om/Annotation> .

<http://www.w3.org/ns/prov#Agent> a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "agent" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass <http://www.w3.org/ns/prov#Agent> ;
            owl:onProperty <http://www.w3.org/ns/prov#actedOnBehalfOf> ],
        [ a owl:Restriction ;
            owl:onClass linkml:String ;
            owl:onProperty <https://w3id.org/kgcl_schema/basics/id> ;
            owl:qualifiedCardinality 1 ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass <http://www.w3.org/ns/prov#Activity> ;
            owl:onProperty <http://www.w3.org/ns/prov#wasInformedBy> ] ;
    skos:definition "a provence-generating agent" ;
    skos:exactMatch <http://www.w3.org/ns/prov#Agent> .

kgcl_schema:ChangeMixin a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "change mixin" ;
    rdfs:subClassOf linkml:mixin ;
    skos:definition "root class for all change mixins" .

kgcl_schema:EdgeChange a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "edge change" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass <https://w3id.org/kgcl_schema/basics/LanguageTag> ;
            owl:onProperty kgcl_schema:language ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass <http://w3id.org/kgcl_schema/om/Edge> ;
            owl:onProperty kgcl_schema:about_edge ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty kgcl_schema:datatype ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty kgcl_schema:object_type ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass <http://w3id.org/kgcl_schema/om/Node> ;
            owl:onProperty <http://w3id.org/kgcl_schema/om/subject> ],
        kgcl_schema:SimpleChange ;
    skos:altLabel "axiom change",
        "triple change" ;
    skos:definition "A change in which the element that is the focus of the change is an edge." .

<http://w3id.org/kgcl_schema/om/Annotation> a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "annotation" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty <http://w3id.org/kgcl_schema/om/property_type> ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass <http://w3id.org/kgcl_schema/om/Annotation> ;
            owl:onProperty <http://w3id.org/kgcl_schema/om/annotation_set> ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty <http://w3id.org/kgcl_schema/om/filler_type> ],
        <http://w3id.org/kgcl_schema/om/PropertyValue> ;
    skos:definition "owl annotations. Not to be confused with annotations sensu GO" ;
    skos:exactMatch owl:Annotation .

<http://w3id.org/kgcl_schema/om/OntologyElement> a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "ontology element" ;
    skos:altLabel "entity" ;
    skos:definition "Any component of an ontology or knowledge graph" .

<http://www.w3.org/ns/prov#Activity> a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "activity" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass <http://www.w3.org/ns/prov#Agent> ;
            owl:onProperty <http://www.w3.org/ns/prov#wasAssociatedWith> ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty <http://www.w3.org/ns/prov#endedAtTime> ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty dcterms:description ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty <http://www.w3.org/ns/prov#startedAtTime> ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass <http://www.w3.org/ns/prov#Activity> ;
            owl:onProperty <http://www.w3.org/ns/prov#wasInformedBy> ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty <http://www.w3.org/ns/prov#used> ],
        [ a owl:Restriction ;
            owl:onClass linkml:String ;
            owl:onProperty <https://w3id.org/kgcl_schema/basics/id> ;
            owl:qualifiedCardinality 1 ] ;
    skos:definition "a provence-generating activity" ;
    skos:exactMatch <http://www.w3.org/ns/prov#Activity> .

kgcl_schema:NodeChange a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "node change" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty kgcl_schema:about_node_representation ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass <https://w3id.org/kgcl_schema/basics/LanguageTag> ;
            owl:onProperty kgcl_schema:language ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass <http://w3id.org/kgcl_schema/om/Node> ;
            owl:onProperty kgcl_schema:about_node ],
        kgcl_schema:SimpleChange ;
    skos:definition "A simple change where the change is about a node" .

kgcl_schema:Change a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "change" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty dcterms:creator ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass kgcl_schema:Change ;
            owl:onProperty kgcl_schema:has_undo ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty kgcl_schema:pull_request ],
        [ a owl:Restriction ;
            owl:onClass linkml:String ;
            owl:onProperty <https://w3id.org/kgcl_schema/basics/id> ;
            owl:qualifiedCardinality 1 ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty dcterms:creator ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty dcterms:date ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty rdfs:seeAlso ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass <http://www.w3.org/ns/prov#Activity> ;
            owl:onProperty <http://www.w3.org/ns/prov#wasGeneratedBy> ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty rdf:type ] ;
    skos:definition "Any change perform on an ontology or knowledge graph" .

kgcl_schema:change_description a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "change description" ;
    rdfs:range linkml:String ;
